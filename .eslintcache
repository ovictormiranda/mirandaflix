nc (movieId, type) => {\r\n      let info = {};\r\n\r\n      if(movieId) {\r\n        switch(type) {\r\n          case 'movie':\r\n            info = await basicFetch(`/movie/${movieId}?language=pt-BR&api_key=${API_KEY}`);\r\n          break;\r\n          \r\n          case 'tv':\r\n            info = await basicFetch(`/tv/${movieId}?language=pt-BR&api_key=${API_KEY}`)\r\n          break;\r\n          \r\n          default:\r\n            info = null;\r\n          break;\r\n        }\r\n      }\r\n\r\n      return info;\r\n    }\r\n\r\n}","C:\\Users\\Drogon\\Desktop\\Clones\\netflix\\src\\components\\MovieRow.js",["33"],"C:\\Users\\Drogon\\Desktop\\Clones\\netflix\\src\\components\\FeaturedMovie.js",["34"],"C:\\Users\\Drogon\\Desktop\\Clones\\netflix\\src\\components\\Header\\index.js",["35"],"import React from 'react';\r\nimport './index.css';\r\n\r\nexport default ({black}) => {\r\n  return(\r\n    <header className={black ? 'black' : ''}>\r\n      <div className=\"header--logo\">\r\n        <a href=\"/\">\r\n          <img src=\"https://fontmeme.com/permalink/201222/db8dcada88985a9dab90bae80bb0adae.png\" alt=\"Netflix\"/>\r\n        </a>\r\n      </div>\r\n      <div className=\"header--user\">\r\n        <a href=\"/\">\r\n          <img src=\"https://pro2-bar-s3-cdn-cf1.myportfolio.com/dddb0c1b4ab622854dd81280840458d3/98032aebff601c1d993e12a0_rw_600.png?h=8030f4d5734548795c22da59ca72e3e1\" alt=\"Usuário\" />\r\n        </a>\r\n      </div>\r\n    </header>\r\n  );\r\n}",{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":8,"column":1,"nodeType":"42","endLine":76,"endColumn":2},{"ruleId":"40","severity":1,"message":"43","line":21,"column":1,"nodeType":"42","endLine":88,"endColumn":2},{"ruleId":"40","severity":1,"message":"41","line":6,"column":1,"nodeType":"42","endLine":51,"endColumn":2},{"ruleId":"40","severity":1,"message":"41","line":4,"column":1,"nodeType":"42","endLine":37,"endColumn":2},{"ru